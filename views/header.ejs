<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8" />
		<title>Wang.Blog</title>
		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
		<meta name="keywords" content="wpl,wpl.wiki" />
		<meta name="description" content="wangpengliang个人笔记" />
		<link rel="stylesheet" href="/layui/css/layui.css" />
		<link rel="stylesheet" href="/css/global.css" />
		<script src="/layui/layui.js"></script>
	</head>
	<body>
		<div class="fly-header layui-bg-black">
			<div class="layui-container">
				<ul class="layui-nav" id="menus">
					<% if (success) { %>
					<h2>
						<%= message %>
					</h2>
					<% var firstMenu =getFirstMenu(data);%>
					<ul>
						<li class="layui-nav-item">
							<a href="case/case.html">
								<i class="iconfont icon-iconmingxinganli"></i>代码示例
							</a>
						</li>
						<% for(var i = 0; i < firstMenu.length; i++) { %>
						<li class="layui-nav-item">
							<a href="javascript()">
								<%= firstMenu[i].name %></a>
							<% var secondMenu = getSecondMenu(data,firstMenu[i].id);%>
							<% if (secondMenu && secondMenu.length>0) { %>
							<dl class="layui-nav-child">
								<% for(var j = 0; j < secondMenu.length; j++) { %>
								<dd><a href="">
										<%=secondMenu[j].name %></a></dd>
								<% } %>
							</dl>
							<% } %>
						</li>
						<% } %>
					</ul>
					<% } %>
				</ul>
				<div></div>
				<div class="fly-panel fly-column">
					<div class="layui-container">
						<ul class="layui-clear">
							<li class="layui-hide-xs layui-this"><a href="/">首页</a></li>
							<li>
								<a href="jie/index.html">讨论<span class="layui-badge-dot"></span></a>
							</li>
							<li><a href="jie/index.html">讨论</a></li>
							<li class="layui-hide-xs layui-hide-sm layui-show-md-inline-block">
								<span class="fly-mid"></span>
							</li>
						</ul>
						<div class="fly-column-right layui-hide-xs">
							<span class="fly-search"><i class="layui-icon"></i></span>
						</div>
					</div>
				</div>
				<script>
					layui
						.config({
							version: "3.0.0",
							base: "/mods/"
						})
						.extend({
							common: "common"
						});
					layui.use(["common", "jquery", "element"], function() {
						var common = layui.common;
						var element = layui.element;
						element.init();
					});
				</script>
			</div>
		</div>
	</body>
